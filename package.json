{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 expo start --clear",
    "web": "cross-env EXPO_NO_TELEMETRY=1 expo start --web --clear",
    "android": "cross-env EXPO_NO_TELEMETRY=1 expo start --android",
    "ios": "cross-env EXPO_NO_TELEMETRY=1 expo start --ios",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "test:supabase": "tsx --no-esm scripts/test-supabase.ts",
    "check:supabase": "tsx --no-esm scripts/check-supabase-connection.ts",
    "explore:supabase": "node scripts/explore-supabase.js",
    "read:supabase": "node scripts/read-supabase-structure.js",
    "query:supabase": "node scripts/query-supabase.js",
    "check:match": "node scripts/check-last-match.js",
    "find:player": "node scripts/find-player.js",
    "check:elo": "node scripts/check-elo-matches.js",
    "test:elo": "node scripts/test-elo-coefficient.js",
    "estimate:first": "node scripts/estimate-victories-to-first.js",
    "simulate:close": "node scripts/simulate-close-victories.js",
    "test:new-elo": "node scripts/test-new-elo-system.js",
    "seed:complete": "node scripts/seed-complete-database.js",
    "seed:existing": "node scripts/seed-with-existing-users.js"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.53.0",
    "dotenv": "^17.2.3",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.5",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.2.2",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3"
  }
}
